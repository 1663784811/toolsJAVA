<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cf.carpark.dao.mapper.${__Table__}Mapper" >
  <!-- ==================  BaseResultMap   =================== -->
  <resultMap id="BaseResultMap" type="com.cf.carpark.domain.${__Table__}" >
    <#list javaColumns as column>
      <#if column.isPrimary>
        <id column="${column.columnName}" property="${operationTools.delSpecial(column.columnName)}" jdbcType="VARCHAR" />
      </#if>
      <#if !column.isPrimary>
        <result column="${column.columnName}" property="${operationTools.delSpecial(column.columnName)}" jdbcType="${column.dbType}" />
      </#if>
    </#list>
  </resultMap>

  <!-- ==================  Example_Where_Clause   =================== -->
  <sql id="Example_Where_Clause" >
    <where >
      <foreach collection="oredCriteria" item="criteria" separator="or" >
        <if test="criteria.valid" >
          <trim prefix="(" suffix=")" prefixOverrides="and" >
            <foreach collection="criteria.criteria" item="criterion" >
              <choose >
                <when test="criterion.noValue" >
                  and ${r'${criterion.condition}'}
                </when>
                <when test="criterion.singleValue" >
                  and ${r'${criterion.condition} #{criterion.value}'}
                </when>
                <when test="criterion.betweenValue" >
                  and ${r'${criterion.condition} #{criterion.value} and #{criterion.secondValue}'}
                </when>
                <when test="criterion.listValue" >
                  and ${r'${criterion.condition}'}
                  <foreach collection="criterion.value" item="listItem" open="(" close=")" separator="," >
                    ${r'#{listItem}'}
                  </foreach>
                </when>
              </choose>
            </foreach>
          </trim>
        </if>
      </foreach>
    </where>
  </sql>

  <!-- ==================  Update_By_Example_Where_Clause   =================== -->
  <sql id="Update_By_Example_Where_Clause" >
    <where >
      <foreach collection="example.oredCriteria" item="criteria" separator="or" >
        <if test="criteria.valid" >
          <trim prefix="(" suffix=")" prefixOverrides="and" >
            <foreach collection="criteria.criteria" item="criterion" >
              <choose >
                <when test="criterion.noValue" >
                  and ${r'${criterion.condition}'}
                </when>
                <when test="criterion.singleValue" >
                  and ${r'${criterion.condition} #{criterion.value}'}
                </when>
                <when test="criterion.betweenValue" >
                  and ${r'${criterion.condition} #{criterion.value} and #{criterion.secondValue}'}
                </when>
                <when test="criterion.listValue" >
                  and ${r'${criterion.condition}'}
                  <foreach collection="criterion.value" item="listItem" open="(" close=")" separator="," >
                    ${r'#{listItem}'}
                  </foreach>
                </when>
              </choose>
            </foreach>
          </trim>
        </if>
      </foreach>
    </where>
  </sql>

  <!-- ==================  Base_Column_List  =================== -->
  <sql id="Base_Column_List" ><#list javaColumns as column>${column.columnName}<#if (javaColumns?size-1)!=column_index>,</#if></#list></sql>

  <!-- ==================  selectByExample  =================== -->
  <select id="selectByExample" resultMap="BaseResultMap" parameterType="com.cf.carpark.domain.${__Table__}Example" >
    select
    <if test="distinct" >
      distinct
    </if>
    <include refid="Base_Column_List" />
        from ${javaData.table}
    <if test="_parameter != null" >
      <include refid="Example_Where_Clause" />
    </if>
    <if test="orderByClause != null" >
      order by ${r'${orderByClause}'}
    </if>
  </select>

  <!-- ==================  selectByPrimaryKey  =================== -->
  <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.String" >
    select
    <include refid="Base_Column_List" />
    from ${javaData.table}
    where ${operationTools.getPK(javaColumns).columnName} = ${r'#{'+operationTools.getPK(javaColumns).columnName+',jdbcType='+operationTools.getPK(javaColumns).dbType+'}'}
  </select>

  <!-- ==================  deleteByPrimaryKey  =================== -->
  <delete id="deleteByPrimaryKey" parameterType="java.lang.String" >
    delete from ${javaData.table}
    where ${operationTools.getPK(javaColumns).columnName} = ${r'#{'+operationTools.getPK(javaColumns).columnName+',jdbcType='+operationTools.getPK(javaColumns).dbType+'}'}
  </delete>

  <!-- ==================  deleteByExample  =================== -->
  <delete id="deleteByExample" parameterType="com.cf.carpark.domain.${__Table__}Example" >
    delete from ${javaData.table}
    <if test="_parameter != null" >
      <include refid="Example_Where_Clause" />
    </if>
  </delete>

  <!-- ==================  insert  =================== -->
  <insert id="insert" parameterType="com.cf.carpark.domain.${__Table__}" >
    insert into ${javaData.table} (<#list javaColumns as column>${column.columnName}<#if (javaColumns?size-1)!=column_index>,</#if></#list>)
    values (
      <#list javaColumns as column>${r'#{'+column.javaField+',jdbcType='+column.dbType+'}'}<#if (javaColumns?size-1)!=column_index>,</#if>
        </#list>
        )
  </insert>

  <!-- ==================  insertSelective  =================== -->
  <insert id="insertSelective" parameterType="com.cf.carpark.domain.${__Table__}" >
    insert into ${javaData.table}
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <#list javaColumns as column>
      <if test="${column.javaField} != null" >
        ${column.columnName},
      </if>
      </#list>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <#list javaColumns as column>
      <if test="${column.javaField} != null" >
        ${r'#{'+column.javaField+',jdbcType='+column.dbType+'},'}
      </if>
      </#list>
    </trim>
  </insert>

  <!-- ==================  countByExample  =================== -->
  <select id="countByExample" parameterType="com.cf.carpark.domain.${__Table__}Example" resultType="java.lang.Integer" >
    select count(*) from ${javaData.table}
    <if test="_parameter != null" >
      <include refid="Example_Where_Clause" />
    </if>
  </select>

  <!-- ==================  updateByExampleSelective  =================== -->
  <update id="updateByExampleSelective" parameterType="map" >
    update ${javaData.table}
    <set >
      <#list javaColumns as column>
      <if test="record.${column.javaField} != null" >
        ${column.columnName} = ${r'#{record.'+column.javaField+',jdbcType='+column.dbType+'}'},
      </if>
      </#list>
    </set>
    <if test="_parameter != null" >
      <include refid="Update_By_Example_Where_Clause" />
    </if>
  </update>

  <!-- ==================  updateByExample  =================== -->
  <update id="updateByExample" parameterType="map" >
    update ${javaData.table}
    set
    <#list javaColumns as column>
        ${column.columnName} = ${r'#{record.'+column.javaField+',jdbcType='+column.dbType+'}'}<#if (javaColumns?size-1)!=column_index>,</#if>
    </#list>
    <if test="_parameter != null" >
      <include refid="Update_By_Example_Where_Clause" />
    </if>
  </update>

  <!-- ==================  updateByPrimaryKeySelective  =================== -->
  <update id="updateByPrimaryKeySelective" parameterType="com.cf.carpark.domain.${__Table__}" >
    update ${javaData.table}
    <set >
      <#list javaColumns as column>
      <#if !column.isPrimary>
      <if test="${column.javaField} != null" >
        ${column.columnName} = ${r'#{'+column.javaField+',jdbcType='+column.dbType+'}'},
      </if>
      </#if>
      </#list>
    </set>
    where  ${operationTools.getPK(javaColumns).columnName} = ${r'#{'+operationTools.getPK(javaColumns).columnName+',jdbcType='+operationTools.getPK(javaColumns).dbType+'}'}
  </update>

  <!-- ==================  updateByPrimaryKey  =================== -->
  <update id="updateByPrimaryKey" parameterType="com.cf.carpark.domain.${__Table__}" >
    update ${javaData.table}
    set
    <#list javaColumns as column>
    <#if !column.isPrimary>
        ${column.columnName} = ${r'#{'+column.javaField+',jdbcType='+column.dbType+'}'}<#if (javaColumns?size-1)!=column_index>,</#if>
    </#if>
    </#list>
    where  ${operationTools.getPK(javaColumns).columnName} = ${r'#{'+operationTools.getPK(javaColumns).columnName+',jdbcType='+operationTools.getPK(javaColumns).dbType+'}'}
  </update>

</mapper>